{
    "compilerOptions": {
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "strict": true,
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "strictNullChecks": false,
        "baseUrl": ".",
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*"
    ]
}